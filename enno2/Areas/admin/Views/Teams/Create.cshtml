@model enno2.Models.Team

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Team</h4>
<hr />
<div class="row">
    <div class="container">
        <form asp-action="Create" method="post" enctype="multipart/form-data">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="mb-3">
                <label asp-for="ImageFile" class="form-label">Image</label>
                <input asp-for="ImageFile" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                <span asp-validation-for="ImageFile" class="text-danger"></span>
            </div>
            <div class="mb-3">
                <label asp-for="Name" class="form-label">Name</label>
                <input asp-for="Name" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="mb-3 form-check">
                <label asp-for="Surname" class="form-label">Surname</label>
                <input asp-for="Surname" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                <span asp-validation-for="Surname" class="text-danger"></span>
            </div>
            <div class="mb-3 form-check">
                <label asp-for="Position" class="form-label">Position</label>
                <input asp-for="Position" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
                <span asp-validation-for="Position" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>

    </div>


</div>

